# Proxy host configs                                          #
#-#-#-#-#-#-#-#-#-#-#-#-#-#-#-#-#-#-#-#-#-#-#-#-#-#-#-#-#-#-#-#

# Match host works in more cases than Host for re-parsed hostnames
Match host *labcomp*,!*jump*,!1.2.*.*
 ProxyCommand ssh jump.us-east-1 -W %h:%p
# # ProxyJump passes along ssh flags, like `-vvv`, to the jump host, but requires a static username to use with the jump host if different from the
# # final host
# ProxyJump %r@jump.us-east-1
# ProxyCommand ssh %r@jump.us-east-1 -W %h:%p

# Match host works in more cases than Host for re-parsed hostnames
Match host *.internal.hostname.com,*.somehost.com,!*jump*
 ProxyCommand ssh jump -W %h:%p
# ProxyJump %r@jump
